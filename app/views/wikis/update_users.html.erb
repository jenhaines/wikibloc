<div class="row">
<div class="col-md-12">
  <h2><%= @wiki.title %> Collaborators</h2>
</div>
</div>
<%= form_for(@wiki) do |f| %>
<div class="row">
  <div class="col-md-2">
             <%= f.submit "Update", class: 'btn btn-primary' %>
    </div>
  <div class="col-md-8">
        <table class="table table-hover">
          <tr>
            <td>?</td>
            <td>Name</td>
            <td>ID</td>
          </tr>
              <tr>
            <% User.all.each do |us| %>
                <td><%= check_box_tag "wiki[user_ids][]", us.id, @wiki.user_ids.include?(us.id), id: dom_id(us) %></td>
                <td><%= label_tag dom_id(us), us.name %></td>
                <td><%= us.id %></td>
              </tr>
            <% end %>
        </table>
          <% end %>
      </div>
    </div>