<div class="row">
  <h1><%= @wikis.count %> WIKIS</h1>
   <div class="col-md-10">
   <div class="input-group">
            <div class="input-group-btn">
              <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">My Wikis <span class="caret"></span></button>
              <ul class="dropdown-menu" role="menu">
                <li><%= link_to "All Wikis", "#" %></li>
                <li><a href="#">Favorites</a></li>
                <li class="divider"></li>
                <li><a href="#">Separated link</a></li>
              </ul>
              
            </div><!-- /btn-group -->
            <input type="text" class="form-control">
          </div><!-- /input-group -->
     <table class="table">
     <tr>
       <td><b>Title</b></td>
       <td><b>Id</b></td>
       <td><b>Author</b></td>
       <td><b>Public</b></td>
       <td><b>Collaborate</b></td>
     </tr>
     <% @wikis.each do |wiki| %>
     <tr><td><%= link_to wiki.title, wiki_path(wiki) %></td>
         <td><%= wiki.id %></td>
         <td><%= wiki.owner.name %></td>
         <td><%= "yes" unless wiki.hide %></td>
         <td><%= "yes" if wiki.users.include?(current_user) %></td>
         </tr>
   
     <% end %>
     </table>
   </div>

   </div>
